{
    "version": 2,
    "name": "subatomic-rocks",
    "alias": "subatomic.rocks",
    "builds": [
        { "src": "./dist/**", "use": "@now/static" }
    ],
    "routes": [
      { "src": "/(.*)", "dest": "dist/$1" }
      { "src": "/sobre", "dest": "dist/" }
      { "src": "/contato", "dest": "dist/" }
    ]
}