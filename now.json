{
    "version": 2,
    "name": "subatomic-rocks",
    "alias": "subatomic.rocks",
    "builds": [
        { "src": "./dist/**", "use": "@now/static" }
    ],
    "routes": [
      { "src": "/(.*)", "dest": "public/$1" },
      { "src": "/sobre", "dest": "public/index.html" },
      { "src": "/contato", "dest": "public/index.html" }
    ]
}